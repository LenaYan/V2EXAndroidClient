<manifest package="com.ray.sample.v2ex"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:name=".V2EXApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name=".view.main.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.v2ex.TopicListActivity"
            android:label="@string/title_activity_topic_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.mock.ListSampleActivity"
            android:label="@string/title_activity_list_sample"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.mock.MockSamplesActivity"
            android:label="@string/title_activity_mock_samples"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.mock.WrapperListSampleActivity"
            android:label="@string/title_activity_wrapper_list_sample"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.mock.PageSampleActivity"
            android:label="@string/title_activity_page_sample"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.v2ex.TopicDetailActivity"
            android:label="@string/title_activity_topic_detail"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.template.BasicTempActivity"
            android:label="@string/title_activity_basic_temp"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>